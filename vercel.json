{
    "version": 2,
    "builds": [
      {
        "src": "src/pages/**/*.{js,ts,jsx,tsx,mdx}",
        "use": "@vercel/node"
      },
      {
        "src": "src/components/**/*.{js,ts,jsx,tsx,mdx}",
        "use": "@vercel/node"
      },
      {
        "src": "src/app/**/*.{js,ts,jsx,tsx,mdx}",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/pages/$1"
      }
    ]
  }
  